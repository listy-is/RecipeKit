      },
      "url": "$INPUT",
      "config": {
        "method": "GET"
      },
      "description": "Retrieves a JSON with output of apple"
    },
    {
      "command": "json_store_text",
      "locator": "results.[0].artistName",
      "input": "$JSON",
      "output": {
        "name": "AUTHOR",
        "type": "string",
        "show": true
      },
      "description": "Saves the artist name"
    },
    {
      "command": "json_store_text",
      "locator": "results.[0].trackName",
      "input": "$JSON",
      "output": {
        "name": "TITLE",
        "type": "string",
        "show": true
      },
      "description": "Saves the title first song"
    },
    {
      "command": "json_store_text",
      "locator": "results.[0].trackViewUrl",
      "input": "$JSON",
      "output": {
        "name": "URL",
        "type": "string",
        "show": true
      },
      "description": "Saves the url."
    },
    {
      "command": "json_store_text",
      "locator": "results.[0].collectionName",
      "input": "$JSON",
      "output": {
        "name": "ALBUM",
        "type": "string",
        "show": true
      },
      "description": "Saves the album"
    },
    {
      "command": "json_store_text",
      "locator": "results.[0].artworkUrl600",
      "input": "$JSON",
      "output": {
        "name": "COVER",
        "type": "string",
        "show": true
      },
      "description": "Saves the cover"
    },
    {
      "command": "json_store_text",
      "locator": "results.[0].trackPrice",
      "input": "$JSON",
      "output": {
        "name": "PRICE",
        "type": "float",
        "show": true
      },
      "description": "Saves the price"
    },
    {
      "command": "json_store_text",
      "locator": "results.[0].releaseDate",
      "input": "$JSON",
      "output": {
        "name": "DATE"
      },
      "description": "Saves the release date"
    },
    {
      "command": "json_store_text",
      "locator": "results.[0].primaryGenreName",
      "input": "$JSON",
      "output": {
        "name": "GENRE",
        "type": "string",
        "show": true
      },
      "description": "Saves the genre"
    },
    {
      "command": "regex",
      "input": "$DATE",
      "expression": "\\d\\d\\d\\d",
      "output": {
        "name": "DATE",
        "type": "date",
        "format": "YYYY",
        "show": true
      },
      "description": "Applies a regular expression to get just the year of the date"
    }
