{
  "recipe_id": 15,
  "version": 7,
  "engine_version": 6,
  "title": "TMDB Movies",
  "description": "A recipe to get a list of movies",
  "urls": [
    "https://www.themoviedb.org/movie/"
  ],
  "headers": {
    "Accept-Language": "es-ES",
    "User-Agent": "Mozilla/5.0 (Linux; Android 10; Pixel 4) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/81.0.4044.129 Mobile Safari/537.36"
  },
  "autocomplete_steps": [
    {
      "command": "load",
      "url": "https://www.themoviedb.org/search/movie?language=es-ES&query=$INPUT",
      "description": "Searches movies by title"
    },
    {
      "command": "store_text",
      "locator": "div.results > div:nth-child(1) div.title h2",
      "output": {
        "name": "TITLE1"
      }
    },
    {
      "command": "store_text",
      "locator": "div.results > div:nth-child(1) div.title > span.release_date",
      "output": {
        "name": "YEAR1"
      }
    },
    {
      "command": "regex",
      "input": "$YEAR1",
      "expression": "(\\s\\d\\d\\d\\d)",
      "output": {
        "name": "YEAR1"
      }
    },
    {
      "command": "store",
      "input": "$TITLE1$YEAR1",
      "output": {
        "name": "TITLE1"
      }
    },
    {
      "command": "store_attribute",
      "locator": "div.results > div:nth-child(1) div.title a",
      "attribute_name": "href",
      "output": {
        "name": "URL1"
      }
    },
    {
      "command": "store",
      "input": "https://www.themoviedb.org$URL1",
      "output": {
        "name": "URL1"
